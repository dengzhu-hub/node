{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "no-console": ["error", { "allow": ["warn", "error", "log"] }],
    "no-underscore-dangle":"off",
    "curly":["error", "multi-line"],
    "no-param-reassign":["error", { "props": false }]
  }
}
